<h3>List of users</h3>
<%= will_paginate @users%>
<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Photo</th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= gravatar_for user%></td>
    <td><%= link_to 'Show', user %></td>
    <td> </td>
    <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>

  </tr>

<% end %>

</table>
<%= will_paginate @users%>
<br />


